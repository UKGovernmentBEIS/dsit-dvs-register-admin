    <ProjectReference Include="..\DVSAdmin.CommonUtility\DVSAdmin.CommonUtility.csproj" />
    <ProjectReference Include="..\DVSAdmin.Data\DVSAdmin.Data.csproj" />
  </ItemGroup>
  <ItemGroup>
