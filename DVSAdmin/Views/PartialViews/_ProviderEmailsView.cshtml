@model ProviderEmailsViewModel

<h3 class="govuk-heading-m">@Model.Heading</h3>
<hr class="govuk-section-break govuk-section-break--s govuk-section-break--visible">
<dl class="govuk-summary-list">
    <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
            Email address of primary <br />contact
        </dt>
        <dd class="govuk-summary-list__value">
            @Model.ProviderContact.PrimaryContactEmail
        </dd>
    </div>
    <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
            Email address of secondary <br />contact
        </dt>
        <dd class="govuk-summary-list__value">
            @Model.ProviderContact.SecondaryContactEmail
        </dd>
    </div>
</dl>
