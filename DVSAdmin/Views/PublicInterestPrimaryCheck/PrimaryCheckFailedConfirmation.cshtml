@model DVSAdmin.BusinessLogic.Models.ServiceDto;
@using Newtonsoft.Json
@using DVSAdmin.Extensions;
@using DVSAdmin.CommonUtility.Models.Enums;
@{
    ViewData["Title"] = "This service has failed the primary public interest check";
    Layout = "~/Views/Shared/_LayoutLoggedIn.cshtml";
}
<div class="govuk-width-container">

    <main id="main-content" class="govuk-main-wrapper" role="main">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-full">
                <h1 class="govuk-heading-xl">
                    This service has failed the primary public interest check
                </h1>
                <div class="govuk-grid-column-full">
                    @await Html.PartialAsync("~/Views/PublicInterestCheck/PartialViews/_ServiceDetailsWithReviewStatus.cshtml", Model)
                </div>

                <p class="govuk-body">
                    This application will be moved to the 'Secondary check' tab. A second OFDIA reviewer will need to approve this decision.
                </p>
                <p class="govuk-body">
                    We will send an email notification about this to the DI Register Notifications inbox, within the next 20 minutes.
                </p>

                <a href="/public-interest-check/public-interest-check-list" type="submit" class="govuk-button" data-module="govuk-button">
                    Go back to public interest check
                </a>
            </div>
        </div>
    </main>
</div>
