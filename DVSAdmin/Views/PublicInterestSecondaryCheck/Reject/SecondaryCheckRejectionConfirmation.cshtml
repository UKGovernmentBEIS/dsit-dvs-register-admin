@model DVSAdmin.BusinessLogic.Models.ServiceDto;
@using DVSAdmin.Extensions;
@using DVSAdmin.CommonUtility.Models.Enums;
@{
    ViewData["Title"] = "You have rejected this application";
    Layout = "~/Views/Shared/_LayoutLoggedIn.cshtml";
}

<div class="govuk-width-container">
   
    <main id="main-content" class="govuk-main-wrapper" role="main">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-full">
                <h1 class="govuk-heading-xl">
                    You have rejected this application
                </h1>

            
                    @await Html.PartialAsync("~/Views/PublicInterestCheck/PartialViews/_ServiceDetailsWithReviewStatus.cshtml", Model)
              

                <p class="govuk-body">
                    We will send an email notification to the DI Register Notifications inbox, within the next 20 minutes.
                </p>
                </div>

                <a href="@Url.Action("PublicInterestCheck","PublicInterestCheck")" role="button" draggable="false" class="govuk-button"
                   data-module="govuk-button">
                    Go back to public interest check
                </a>

           
        </div>
    </main>
</div>

